{
  "name": "api-node-clean",
  "version": "1.0.0",
  "description": "api nodejs with clean architecture",
  "main": "index.js",
  "scripts": {
    "test": "jest --noStackTrace --passWithNoTests --detectOpenHandles",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests --silent",
    "test:ci": "npm test -- --coverage --silent"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauloalchemist/api-node-clean.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "express"
  ],
  "author": "Paulo Lins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pauloalchemist/api-node-clean/issues"
  },
  "homepage": "https://github.com/pauloalchemist/api-node-clean#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "validator": "^13.5.2"
  }
}
